<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.one.doo.riskoffounndation.mapper.RiskOfFoundationMapper">
<select id="read" resultType="com.one.doo.riskoffounndation.domain.RiskOfFoundation">
